                        android:layout_width="@dimen/_40sdp"
                        android:layout_height="@dimen/_40sdp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_centerInParent="true"
                        android:src="@drawable/training_bg"
                        app:civ_border_color="@color/border_color1"
                        app:civ_border_width="@dimen/_3sdp" />

                    <TextView
                        android:id="@+id/tv_player3"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_40sdp"
                        android:layout_above="@+id/ci_player4"
                        android:layout_alignBaseline="@+id/ci_player3"
                        android:layout_centerInParent="true"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:layout_toRightOf="@+id/ci_player3"
                        android:fontFamily="@font/poppins_bold"
                        android:gravity="center|start"
                        android:singleLine="true"
                        android:text=""
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_20sdp"
                        android:textStyle="bold" />

                    <TextView
